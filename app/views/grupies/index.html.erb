<p id="notice"><%= notice %></p>

<h1>Listing Grupies</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @grupies.each do |grupy| %>
      <tr>
        <td><%= link_to 'Show', wykladowca_grupy_path(grupy) %></td>
        <td><%= link_to 'Edit', edit_wykladowca_grupy_path(grupy) %></td>
        <td><%= link_to 'Destroy', wykladowca_grupy_path, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Grupy', new_wykladowca_grupy_path(:user_id) %>
