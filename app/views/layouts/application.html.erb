      <%= link_to "Log out", logout_path, method: "delete" %>
      <%if zalogowany_wykladowca? %>
      <%= link_to 'WrÃ³c do panelu', wykladowca_path(session[:user_id]) %>
      <%end%>
      <%= yield %>
      <%= debug(params) if Rails.env.development? %>
    </div>
  </body>
</html>